#include <iostream>
using namespace std;

int main() {
string name;
    int age;
    int born;
    int today  = 2025;


    cout <<"What's your name : ";
    cin >> name;

    cout <<"how old are you? ";
    cin >> age;
cout <<"=== Output ===== " << endl;
    cout <<"Hello " << name << endl;

    born = today - age;
    cout <<"You were born in : " << born << endl;
    cout << "Thanks for using our coding ";


    return 0;
}