-- Switch to the database 'mydb' to make sure all operations are in this database
USE mydb;

-- Insert a single row into the 'employees' table
-- Here, only employee_id, first_name, and last_name are provided
-- If other columns exist (like hourly_pay, hire_date), they will be NULL
INSERT INTO employees (employee_id, first_name, last_name)
VALUES (6, 'Hell', 'Plan');

-- Insert multiple rows into the 'employees' table
-- Each set of values corresponds to a row: (employee_id, first_name, last_name, hourly_pay, hire_date)
INSERT INTO employees (employee_id, first_name, last_name, hourly_pay, hire_date)
VALUES
    (1, 'Mohammed', 'Alrashadi', 15.00, '2024-02-03'),   -- first employee
    (2, 'Sara', 'Ahmed', 18.50, '2023-12-15'),           -- second employee
    (3, 'Ali', 'Hassan', 20.00, '2024-01-10'),           -- third employee
    (4, 'Lina', 'Khalid', 16.75, '2024-03-01');          -- fourth employee

-- Select all rows from the 'employees' table to view the current data
SELECT * FROM employees;